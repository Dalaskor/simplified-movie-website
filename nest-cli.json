{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/main_app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/main_app/tsconfig.app.json",
    "plugins": [
      "@nestjs/swagger"
    ]
  },
  "monorepo": true,
  "root": "apps/main_app",
  "projects": {
    "main_app": {
      "type": "application",
      "root": "apps/main_app",
      "entryFile": "main",
      "sourceRoot": "apps/main_app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main_app/tsconfig.app.json"
      }
    },
    "film": {
      "type": "application",
      "root": "apps/film",
      "entryFile": "main",
      "sourceRoot": "apps/film/src",
      "compilerOptions": {
        "tsConfigPath": "apps/film/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "staff": {
      "type": "application",
      "root": "apps/staff",
      "entryFile": "main",
      "sourceRoot": "apps/staff/src",
      "compilerOptions": {
        "tsConfigPath": "apps/staff/tsconfig.app.json"
      }
    },
    "genre": {
      "type": "application",
      "root": "apps/genre",
      "entryFile": "main",
      "sourceRoot": "apps/genre/src",
      "compilerOptions": {
        "tsConfigPath": "apps/genre/tsconfig.app.json"
      }
    },
    "country": {
      "type": "application",
      "root": "apps/country",
      "entryFile": "main",
      "sourceRoot": "apps/country/src",
      "compilerOptions": {
        "tsConfigPath": "apps/country/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    }
  }
}